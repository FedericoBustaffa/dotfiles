{
    "modules-left": [
        "hyprland/workspaces"
    ],
    "modules-center": [
        "hyprland/window"
    ],
    "hyprland/workspaces": {
        "disable-scroll": true,
        "all-outputs": true,
        "warp-on-scroll": false,
        "on-click": "activate",
        "format": "{name}",
        "show-empty": false,
    },
    "hyprland/window": {
        "format": "{class}",
        "icon": true,
        "icon-size": 20
    },
    "include": "~/.config/waybar/config.jsonc"
}

